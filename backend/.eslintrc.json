{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "no-underscore-dangle": "off"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "overrides": [
    {
      "files": [
        "*.js"
      ],
      "rules": {
        "indent": [
          "error",
          2
        ]
      }
    }
  ]
}